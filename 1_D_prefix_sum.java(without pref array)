class Solution {
    public ArrayList<Integer> prefSum(int[] arr) {
        ArrayList<Integer>list=new ArrayList<>();
        int n=arr.length;
        
        for(int i=1;i<n;i++){
            arr[i]=arr[i-1]+arr[i];
            
        }
        for(int num:arr){
            list.add(num);
        }
        return list;
        
        
        
        
        
        
    }
}
